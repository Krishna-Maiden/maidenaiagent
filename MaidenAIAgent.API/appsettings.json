{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AgentSettings": {
    "EnableAllTools": true,
    "DefaultEnabledTools": ["Search", "Calculator", "Weather"],
    "MaxConcurrentTools": 3,
    "DefaultTimeoutSeconds": 30
  }
}
